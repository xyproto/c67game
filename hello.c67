export *

hello = {
    println("Hello from c67game!")
}

greet = (name) -> {
    printf("Hello, %s!\n", name)
}

square = (x) -> {
    ret x * x
}

add = (a, b) -> {
    ret a + b
}

double = (x) -> {
    ret x * 2
}

countdown = (n) -> {
    i = 0
    @ i < n max 100 {
        println(n - i)
        i = i + 1
    }
    println("Blast off!")
}
