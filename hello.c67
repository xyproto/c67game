export *

hello = {
    println("Hello from c67game!")
}

greet = (name) -> {
    println("Hello, " + name + "!")
}

countdown = (n) -> {
    @ i in n + 1 max 100 {
        println(n - i)
    }
    println("Blast off!")
}

square = (x) -> {
    ret x * x
}

add = (a, b) -> {
    ret a + b
}

double = (x) -> {
    ret x * 2
}
