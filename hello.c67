export *

hello = {
    println("Hello from c67game!")
}

greet = (name: str) -> {
    println("Hello, " + name + "!")
}

countdown = (n: num) -> {
    @ i in n + 1 max 100 {
        println(n - i)
    }
    println("Blast off!")
}

square = (x: num) -> num {
    ret x * x
}

add = (a: num, b: num) -> num {
    ret a + b
}
